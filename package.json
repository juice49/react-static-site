{
  "name": "react-static-site",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "config": {
    "browserify": "index.js -t babelify -o public/dist/index.js"
  },
  "babel": {
    "plugins": [
      "transform-react-jsx",
      "transform-object-rest-spread"
    ],
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "dev": "npm run serve & watchify $npm_package_config_browserify",
    "dist": "NODE_ENV=production browserify $npm_package_config_browserify",
    "serve": "list public -s",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ash",
  "license": "ISC",
  "dependencies": {
    "dateformat": "^1.0.12",
    "front-matter": "^2.1.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.1",
    "react-display-name": "^0.2.0",
    "react-dom": "^15.3.1",
    "react-markdown": "^2.4.2",
    "react-router": "^4.0.0-alpha.3"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "micro-list": "^0.7.0",
    "watchify": "^3.7.0"
  }
}

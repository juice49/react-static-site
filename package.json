{
  "name": "react-static-site",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "babel": {
    "plugins": [
      "transform-react-jsx",
      "transform-object-rest-spread",
      "transform-async-to-generator"
    ],
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "start": "npm run serve",
    "_dev": "npm run build-container & npm run serve & watchify $npm_package_config_browserify --full-paths",
    "_dist": "NODE_ENV=production babel-node scripts/js.js",
    "dist": "babel-node scripts/js.js",
    "read": "babel-node scripts/read",
    "_serve": "st -d public -i index",
    "serve": "babel-node scripts/serve",
    "build": "babel-node scripts/build",
    "build-container": "babel-node scripts/build-container --title 'React Static Site' > public/index",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ash",
  "license": "ISC",
  "dependencies": {
    "aphrodite": "^0.5.0",
    "dateformat": "^1.0.12",
    "detect-node": "^2.0.3",
    "fg-loadjs": "^1.0.0",
    "front-matter": "^2.1.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.1",
    "react-display-name": "^0.2.0",
    "react-dom": "^15.3.1",
    "react-markdown": "^2.4.2",
    "react-router": "^4.0.0-alpha.3"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "basename": "^0.1.2",
    "browserify": "^13.1.1",
    "factor-bundle": "^2.5.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "st": "^1.2.0",
    "watchify": "^3.7.0"
  }
}
